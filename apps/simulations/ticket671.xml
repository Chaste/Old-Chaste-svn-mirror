<?xml version="1.0" encoding="UTF-8"?>
<ChasteParameters
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:noNamespaceSchemaLocation="../../heart/src/io/ChasteParameters.xsd">
       
	<Simulation>       
		<!-- 
			Problem definition 
		-->    
	    <SimulationDuration>5.0</SimulationDuration>
	    <Domain>Mono</Domain>
	    <IonicModel>FaberRudy2000</IonicModel>
	    
		<!-- 
			Mesh definition 
		-->    
		<Mesh>
			<LoadMesh name="apps/simulations/upf-mesh/Cubic075mm" conductivity_media="Axisymmetric"/> 
	  	</Mesh>
		
	    <!-- 
	    	Stimuli (as many <Stimulus> definitions as needed) 
	   	-->
	   	<Stimuli>
		  	<Stimulus> <!-- #1 -->
				<Strength unit="uA/cm^3">-35500.0</Strength>
				<Duration unit="ms">0.5</Duration>
				<Delay unit="ms">0</Delay>
				<Location unit="cm">
					<Cuboid>
						<LowerCoordinates x="-9" y="-2.5" z="-1.5"/>
						<UpperCoordinates x="-8" y="-1.5" z="-0.5"/>
					</Cuboid>
				</Location>
			</Stimulus>
		</Stimuli>	
	
		
		<!-- 
			Output 
		-->
		<OutputDirectory>Ticket671</OutputDirectory>
	</Simulation>	    

	<Physiological>
	</Physiological>

	<Numerical>
		<TimeSteps ode="0.02" pde="0.02" printing="0.02" unit="ms"/>
		<KSPTolerances>
			<KSPRelative>1e-6</KSPRelative>
		</KSPTolerances>
		<KSPSolver>gmres</KSPSolver>
		<KSPPreconditioner>jacobi</KSPPreconditioner>	
	</Numerical>		
	
</ChasteParameters>
