<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<ChasteParameters xmlns="https://chaste.comlab.ox.ac.uk/nss/parameters/1_2">

  <Simulation>
    <SpaceDimension>3</SpaceDimension>
    <SimulationDuration unit="ms">1</SimulationDuration>
    <Domain>Mono</Domain>
    <Mesh unit="cm">
      <LoadMesh conductivity_media="NoFibreOrientation" name="mesh/test/data/1D_0_to_1mm_10_elements"/>
    </Mesh>
    <IonicModels>
      <Default>LuoRudyI</Default>
    </IonicModels>
    <OutputDirectory>BiProblemArchiveHelper</OutputDirectory>
    <OutputFilenamePrefix>BidomainLR91_1d</OutputFilenamePrefix>
  </Simulation>

  <Physiological>
    <IntracellularConductivities longi="0.0005" normal="0" trans="0" unit="mS/cm"/>
    <ExtracellularConductivities longi="0.0005" normal="0" trans="0" unit="mS/cm"/>
    <BathConductivity unit="mS/cm">7</BathConductivity>
    <SurfaceAreaToVolumeRatio unit="1/cm">1</SurfaceAreaToVolumeRatio>
    <Capacitance unit="uF/cm^2">1</Capacitance>
  </Physiological>

  <Numerical>
    <TimeSteps ode="0.01" pde="0.01" printing="0.01" unit="ms"/>
    <KSPTolerances>
      <KSPAbsolute>0.0002</KSPAbsolute>
    </KSPTolerances>
    <KSPSolver>cg</KSPSolver>
    <KSPPreconditioner>bjacobi</KSPPreconditioner>
  </Numerical>

  <PostProcessing/>

</ChasteParameters>
