<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<ChasteParameters xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="/home/chaste/eclipse/workspace/Chaste/heart/src/io/ChasteParameters.xsd">

  <Simulation>
    <SimulationDuration unit="ms">10</SimulationDuration>
    <Domain>Bi</Domain>
    <Mesh unit="cm">
      <LoadMesh conductivity_media="NoFibreOrientation" name="mesh/test/data/cube_1626_elements"/>
    </Mesh>
    <IonicModels>
      <Default>Fox2002BackwardEuler</Default>
    </IonicModels>
    <Stimuli>
      <Stimulus>
        <Strength unit="uA/cm^3">-80000</Strength>
        <Duration unit="ms">1</Duration>
        <Delay unit="ms">0</Delay>
        <Location unit="cm">
          <Cuboid>
            <LowerCoordinates x="0" y="0" z="0"/>
            <UpperCoordinates x="0.1" y="1" z="1"/>
          </Cuboid>
        </Location>
      </Stimulus>
    </Stimuli>
    <OutputDirectory>SaveBidomain</OutputDirectory>
    <OutputFilenamePrefix>BidomainLR91_1d</OutputFilenamePrefix>
    <SaveSimulation></SaveSimulation>
  </Simulation>

  <Physiological>
    <IntracellularConductivities longi="1.75" normal="1.75" trans="1.75" unit="mS/cm"/>
    <ExtracellularConductivities longi="7" normal="7" trans="7" unit="mS/cm"/>
    <SurfaceAreaToVolumeRatio unit="1/cm">1400</SurfaceAreaToVolumeRatio>
    <Capacitance unit="uF/cm^2">1</Capacitance>
  </Physiological>

  <Numerical>
    <TimeSteps ode="0.01" pde="0.1" printing="0.1" unit="ms"/>
    <KSPTolerances>
      <KSPAbsolute>0.0002</KSPAbsolute>
    </KSPTolerances>
    <KSPSolver>gmres</KSPSolver>
    <KSPPreconditioner>jacobi</KSPPreconditioner>
  </Numerical>

</ChasteParameters>
