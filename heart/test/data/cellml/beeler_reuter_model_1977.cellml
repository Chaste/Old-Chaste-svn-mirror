                              </apply>
                           </apply>
                        </apply>
                        <apply>
                           <exp/>
                           <apply>
                              <times/>
                              <cn cellml:units="per_mV">0.04</cn>
                              <apply>
                                 <plus/>
                                 <ci>V</ci>
                                 <cn cellml:units="mV">53</cn>
                              </apply>
                           </apply>
                        </apply>
                     </apply>
                  </apply>
                  <apply>
                     <divide/>
                     <apply>
                        <times/>
