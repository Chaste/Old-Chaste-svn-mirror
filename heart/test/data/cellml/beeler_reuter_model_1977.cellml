      <math xmlns="http://www.w3.org/1998/Math/MathML">
         <apply>
            <eq/>
            <ci>i_K1</ci>
            <apply>
               <times/>
               <cn cellml:units="uA_per_mm2">0.0035</cn>
               <apply>
                  <plus/>
                  <apply>
                     <divide/>
                     <apply>
                        <times/>
                        <cn cellml:units="dimensionless">4</cn>
                        <apply>
                           <minus/>
                           <apply>
