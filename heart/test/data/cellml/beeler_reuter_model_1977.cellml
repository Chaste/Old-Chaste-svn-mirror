                        </apply>
                     </apply>
                     <apply>
                        <minus/>
                        <cn cellml:units="dimensionless">1</cn>
                        <apply>
                           <exp/>
                           <apply>
                              <times/>
                              <apply>
                                 <minus/>
                                 <cn cellml:units="per_mV">0.04</cn>
                              </apply>
                              <apply>
                                 <plus/>
                                 <ci>V</ci>
