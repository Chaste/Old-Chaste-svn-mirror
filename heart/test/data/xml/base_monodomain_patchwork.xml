<?xml version="1.0" encoding="UTF-8"?>
<ChasteParameters xmlns="https://chaste.comlab.ox.ac.uk/nss/parameters/2_0">

	<Simulation>       
		<!-- 
			Problem definition 
		-->    
	    <SimulationDuration unit="ms">0.1</SimulationDuration>
	    <Domain>Mono</Domain>
	    <IonicModels>
	    	<Default><Hardcoded>Fox2002BackwardEuler</Hardcoded></Default>
			<Region> <!-- REGION 1 -->
                <IonicModel><Hardcoded>LuoRudyI</Hardcoded></IonicModel>
                <Location unit="cm">
                    <Cuboid>
                        <LowerCoordinates x="0.0" y="0.0" z="0.0"/>
                        <UpperCoordinates x="0.0" y="0.0" z="0.0"/>
                    </Cuboid>
                </Location>
            </Region><!-- REGION 1 -->
			<Region> <!-- REGION 2 -->
                <IonicModel><Hardcoded>LuoRudyIBackwardEuler</Hardcoded></IonicModel>
                <Location unit="cm">
                    <Cuboid>
                        <LowerCoordinates x="0.1" y="0.0" z="0.0"/>
                        <UpperCoordinates x="0.1" y="0.0" z="0.0"/>
                    </Cuboid>
                </Location>
            </Region><!-- REGION 2 -->
			<Region> <!-- REGION 3 -->
                <IonicModel><Hardcoded>LuoRudyI</Hardcoded></IonicModel>
                <Location unit="cm">
                    <Cuboid>
                        <LowerCoordinates x="0.2" y="0.0" z="0.0"/>
                        <UpperCoordinates x="0.2" y="0.0" z="0.0"/>
                    </Cuboid>
                </Location>
            </Region><!-- REGION 3 -->
			<Region> <!-- REGION 4 -->
                <IonicModel><Hardcoded>FaberRudy2000Optimised</Hardcoded></IonicModel>
                <Location unit="cm">
                    <Cuboid>
                        <LowerCoordinates x="0.3" y="0.0" z="0.0"/>
                        <UpperCoordinates x="0.3" y="0.0" z="0.0"/>
                    </Cuboid>
                </Location>
            </Region><!-- REGION 4 -->
			<Region> <!-- REGION 5 -->
                <IonicModel><Hardcoded>FaberRudy2000</Hardcoded></IonicModel>
                <Location unit="cm">
                    <Cuboid>
                        <LowerCoordinates x="0.4" y="0.0" z="0.0"/>
                        <UpperCoordinates x="0.4" y="0.0" z="0.0"/>
                    </Cuboid>
                </Location>
            </Region><!-- REGION 5 -->
			<Region> <!-- REGION 6 -->
                <IonicModel><Hardcoded>DifrancescoNoble</Hardcoded></IonicModel>
                <Location unit="cm">
                    <Cuboid>
                        <LowerCoordinates x="0.5" y="0.0" z="0.0"/>
                        <UpperCoordinates x="0.5" y="0.0" z="0.0"/>
                    </Cuboid>
                </Location>
            </Region><!-- REGION 6 -->
			<Region> <!-- REGION 7 -->
                <IonicModel><Hardcoded>MahajanShiferaw</Hardcoded></IonicModel>
                <Location unit="cm">
                    <Cuboid>
                        <LowerCoordinates x="0.6" y="0.0" z="0.0"/>
                        <UpperCoordinates x="0.6" y="0.0" z="0.0"/>
                    </Cuboid>
                </Location>
            </Region><!-- REGION 7 -->
			<Region> <!-- REGION 8 -->
                <IonicModel><Hardcoded>HodgkinHuxley</Hardcoded></IonicModel>
                <Location unit="cm">
                    <Cuboid>
                        <LowerCoordinates x="0.7" y="0.0" z="0.0"/>
                        <UpperCoordinates x="0.7" y="0.0" z="0.0"/>
                    </Cuboid>
                </Location>
            </Region><!-- REGION 8 -->
			<Region> <!-- REGION 9 -->
                <IonicModel><Hardcoded>Maleckar</Hardcoded></IonicModel>
                <Location unit="cm">
                    <Cuboid>
                        <LowerCoordinates x="0.8" y="0.0" z="0.0"/>
                        <UpperCoordinates x="0.8" y="0.0" z="0.0"/>
                    </Cuboid>
                </Location>
            </Region><!-- REGION 9 -->
            <Region> <!-- REGION 10 -->
                <IonicModel><Dynamic>
                    <Path relative_to="chaste_source_root">heart/dynamic/libDynamicallyLoadableLr91.so</Path>
                </Dynamic></IonicModel>
                <Location unit="cm">
                    <Cuboid>
                        <LowerCoordinates x="0.9" y="0.0" z="0.0"/>
                        <UpperCoordinates x="1.0" y="0.0" z="0.0"/>
                    </Cuboid>
                </Location>
            </Region>
            <Region> <!-- REGION 11 -->
                <IonicModel><Dynamic>
                    <Path relative_to="chaste_test_output">DynamicallyLoadedModel/luo_rudy_1991_dyn.cellml</Path>
                </Dynamic></IonicModel>
                <Location unit="cm">
                    <Cuboid>
                        <LowerCoordinates x="1.1" y="0.0" z="0.0"/>
                        <UpperCoordinates x="1.2" y="0.0" z="0.0"/>
                    </Cuboid>
                </Location>
            </Region>
        </IonicModels>
	    
		<!-- 
			Mesh definition 
		-->    
		<Mesh unit="cm">
			<Slab x="1.5" y="0.1" z="0.1" inter_node_space="0.1"/>
	  	</Mesh>
		    
	    <!-- 
	    	Stimuli (as many <Stimulus> definitions as needed) 
	   	-->
   		<Stimuli>
			<Stimulus> <!-- #1 -->
				<Strength unit="uA/cm^3">0.0</Strength> <!-- It's not really stimulated -->
				<Duration unit="ms">1.0</Duration>
				<Delay unit="ms">2.0</Delay>
				<Location unit="cm">
					<Cuboid>
						<LowerCoordinates x="-0.05" y="-0.05" z="-0.05"/>
						<UpperCoordinates x="0.05" y="0.05" z="0.05"/>
					</Cuboid>
				</Location>
			</Stimulus>	
		</Stimuli>
        <CellHeterogeneities>
            <CellHeterogeneity> <!-- #1 -->
                <ScaleFactorGks unit="dimensionless">0.462</ScaleFactorGks>
                <ScaleFactorIto unit="dimensionless">0.0</ScaleFactorIto>
                <ScaleFactorGkr unit="dimensionless">1.0</ScaleFactorGkr>
                <Location unit="cm">
                    <Cuboid>
                        <LowerCoordinates x="0.0" y="-0.1" z="-1.0"/>
                        <UpperCoordinates x="10.0" y="0.1" z="1.0"/>
                    </Cuboid>
                </Location>
            </CellHeterogeneity>    
 		</CellHeterogeneities>    
		
		<!-- 
			Output 
		-->
		<OutputDirectory>Patchwork</OutputDirectory>
		<OutputFilenamePrefix>SimulationResults</OutputFilenamePrefix>
	</Simulation>
	
	<Physiological>
	    <!--
	        Conductivities
	    -->
	    <IntracellularConductivities longi="1.75" trans="1.75" normal="1.75" unit="mS/cm"/>
	    <ExtracellularConductivities longi="7.0"  trans="7.0"  normal="7.0" unit="mS/cm"/>
	    <SurfaceAreaToVolumeRatio unit="1/cm"> 1400 </SurfaceAreaToVolumeRatio>
	    <Capacitance unit="uF/cm^2"> 1.0 </Capacitance>
	</Physiological>

	<Numerical>
		<TimeSteps ode="0.01" pde="0.1" printing="0.1" unit="ms"/>
		<KSPTolerances>
			<KSPRelative>1e-6</KSPRelative>
		</KSPTolerances>
		<KSPSolver>gmres</KSPSolver>
		<KSPPreconditioner>jacobi</KSPPreconditioner>
	</Numerical>		
		    
</ChasteParameters>
