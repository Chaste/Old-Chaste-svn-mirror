<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<ChasteParameters xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="/home/chaste/eclipse/workspace/Chaste/heart/src/io/ChasteParameters.xsd">

  <Simulation>
    <SpaceDimension>3</SpaceDimension>
    <SimulationDuration unit="ms">0.1</SimulationDuration>
    <Domain>Mono</Domain>
    <IonicModels>
      <Default>LuoRudyI</Default>
    </IonicModels>
    <OutputDirectory>Monodomain2d</OutputDirectory>
    <OutputFilenamePrefix>monodomain2d</OutputFilenamePrefix>
  </Simulation>

  <Physiological>
    <IntracellularConductivities longi="0.0005" normal="0" trans="0.0005" unit="mS/cm"/>
    <ExtracellularConductivities longi="7" normal="7" trans="7" unit="mS/cm"/>
    <BathConductivity unit="mS/cm">7</BathConductivity>
    <SurfaceAreaToVolumeRatio unit="1/cm">1</SurfaceAreaToVolumeRatio>
    <Capacitance unit="uF/cm^2">1</Capacitance>
  </Physiological>

  <Numerical>
    <TimeSteps ode="0.01" pde="0.01" printing="0.01" unit="ms"/>
    <KSPTolerances>
      <KSPAbsolute>0.0002</KSPAbsolute>
    </KSPTolerances>
    <KSPSolver>cg</KSPSolver>
    <KSPPreconditioner>bjacobi</KSPPreconditioner>
  </Numerical>

  <PostProcessing/>

</ChasteParameters>
